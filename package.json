{
  "name": "dappv1",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.6",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@openzeppelin/contracts-upgradeable": "^4.5.2",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.2.1",
    "@testing-library/user-event": "^12.0.2",
    "@types/jest": "^25.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "eth-components": "^3.0.39",
    "eth-hooks": "^4.0.45",
    "framer-motion": "^4.0.0",
    "numeral": "^2.0.6",
    "qs": "^6.10.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-query": "^3.34.16",
    "react-router-dom": "^6.2.2",
    "react-scripts": "5.0.0",
    "shx": "^0.3.4",
    "ts-node": "^10.7.0",
    "typechain": "^8.0.0",
    "typescript": "^4.5.5",
    "web-vitals": "^0.2.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "tsc --noEmit && eslint src/*",
    "prepare": "husky install",
    "lint-staged": "lint-staged --config lint-staged.js",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "chain": "cd hardhat-ts && npx hardhat node --network hardhat --no-deploy",
    "fork": "cd hardhat-ts && npx hardhat node --fork https://mainnet.infura.io/v3/460f40a260564ac4a4f4b3fffb032dad",
    "node": "cd hardhat-ts && npx hardhat chain",
    "hardhat-test": "cd hardhat-ts && npx hardhat test",
    "compile": "cd hardhat-ts && npx hardhat compile",
    "contracts:build": "cd hardhat-ts && npx hardhat compile && cd .. && npx shx rm -rf './src/generated/external-contracts' && npx shx cp -r './src/scripts/eth-sdk-config.ts.bak' './src/generated/eth-sdk.config.ts' && npx eth-sdk -p ./src/generated",
    "contracts:rebuild": "cd hardhat-ts && npx hardhat hardhat clean && yarn contracts:build",
    "export": "cd hardhat-ts && npx hardhat export",
    "deploy": "cd hardhat-ts && npx hardhat deploy --export-all ../src/generated/hardhat_contracts.json",
    "watch": "cd hardhat-ts && npx hardhat watch",
    "accounts": "cd hardhat-ts && npx hardhat accounts",
    "balance": "cd hardhat-ts && npx hardhat balance",
    "send": "cd hardhat-ts && npx hardhat send",
    "generate": "cd hardhat-ts && npx hardhat generate",
    "account": "cd hardhat-ts && npx hardhat account",
    "mine": "cd hardhat-ts && npx hardhat mine",
    "wallet": "cd hardhat-ts && npx hardhat wallet",
    "fundedwallet": "cd hardhat-ts && npx hardhat fundedwallet",
    "flatten": "cd hardhat-ts && npx hardhat flatten",
    "clean": "cd hardhat-ts && npx hardhat clean"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@chakra-ui/storybook-addon": "^1.0.3",
    "@dethcrypto/eth-sdk": "^0.3.1",
    "@dethcrypto/eth-sdk-client": "^0.1.6",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/node-logger": "^6.4.19",
    "@storybook/preset-create-react-app": "^4.0.1",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@types/numeral": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "tsc-files": "^1.1.3",
    "webpack": "^5.70.0"
  }
}
