  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "tsc --noEmit && eslint src/*",
    "prepare": "husky install",
    "lint-staged": "lint-staged --config lint-staged.js",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "chain": "cd hardhat-ts && npx hardhat node --network hardhat --no-deploy",
    "fork": "cd hardhat-ts && yarn fork",
    "node": "cd hardhat-ts && npx hardhat chain",
    "hardhat-test": "cd hardhat-ts && npx hardhat test",
    "hardhat-yarn-install": "cd hardhat-ts && yarn install",
    "compile": "cd hardhat-ts && npx hardhat compile",
    "contracts:build": "cd hardhat-ts && npx hardhat compile && cd .. && npx shx rm -rf './src/generated/external-contracts' && npx shx cp -r './src/scripts/eth-sdk-config.ts.bak' './src/generated/eth-sdk.config.ts' && npx eth-sdk -p ./src/generated",
    "contracts:rebuild": "cd hardhat-ts && npx hardhat clean && cd .. && yarn contracts:build",
    "export": "cd hardhat-ts && npx hardhat export",
    "deploy": "cd hardhat-ts && npx hardhat deploy --export-all ../src/generated/hardhat_contracts.json --network localhost && cp -R ../src/generated/deployments ./generated/deployments",
    "watch": "cd hardhat-ts && npx hardhat watch",
    "accounts": "cd hardhat-ts && npx hardhat accounts",
    "balance": "cd hardhat-ts && npx hardhat balance",
    "send": "cd hardhat-ts && npx hardhat send",
    "generate": "cd hardhat-ts && npx hardhat generate",
    "account": "cd hardhat-ts && npx hardhat account",
    "mine": "cd hardhat-ts && npx hardhat mine",
    "wallet": "cd hardhat-ts && npx hardhat wallet",
    "fundedwallet": "cd hardhat-ts && npx hardhat fundedwallet",
    "flatten": "cd hardhat-ts && npx hardhat flatten",
    "clean": "cd hardhat-ts && npx hardhat clean",
    "fund": "cd hardhat-ts && npx hardhat fund --network localhost",
    "fund-10": "cd hardhat-ts && npx hardhat fund --amount 10 --network localhost",
    "mint": "cd hardhat-ts && npx hardhat --network localhost mint",
    "deposit-eth": "cd hardhat-ts && npx hardhat --network localhost deposit-eth",
    "withdraw-eth": "cd hardhat-ts && npx hardhat --network localhost withdraw-eth",
    "create-offer": "cd hardhat-ts && npx hardhat --network localhost create-offer",
    "setup-local-chain": "yarn clean && yarn compile && yarn contracts:rebuild && yarn deploy",
    "setup-offer": "yarn deposit-eth && yarn create-offer"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@chakra-ui/storybook-addon": "^1.0.3",
    "@dethcrypto/eth-sdk": "^0.3.1",
    "@dethcrypto/eth-sdk-client": "^0.1.6",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
